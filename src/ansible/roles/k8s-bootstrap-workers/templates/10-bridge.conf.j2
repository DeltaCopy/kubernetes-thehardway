{
    "cniVersion": "0.4.0",
    "name": "bridge",
    "type": "bridge",
    "bridge": "cnio0",
    "isGateway": true,
    "ipMasq": true,
    "ipam": {
        "type": "host-local",
        "ranges": [
          [{"subnet": {% for item in pod_cidr %}{% for key,value in item.items() %}{% if key == inventory_hostname %}"{{ value }}"{% endif %}{% endfor %}{% endfor %}}]
        ],
        "routes": [{"dst": "0.0.0.0/0"}]
    }
}
